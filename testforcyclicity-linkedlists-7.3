ListNode* HasCycle(ListNode* head) {
	ListNode* slow = head;
	ListNode* fast = head;

	while (fast && fast->next) {

	slow = slow->next;
	fast = fast->next->next;

		if (fast == slow) {
			slow = head;

			while (slow != fast) {
				fast = fast->next;
				slow = slow->next;
			}

			return slow;
		}
	}

	return nullptr;
}