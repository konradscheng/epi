int buysellstockonce(vector<int> prices) {

	int maxProfit = 0;
	int runningMin = INT_MAX;

	for (int i = 0; i < prices.size(); i++) {

		if (prices[i] - runningMin > maxProfit) {
			maxProfit = prices[i] - runningMin;
		}

		if (prices[i] < runningMin) {
			runningMin = prices[i];
		}
	}

	return maxProfit;
}