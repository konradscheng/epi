int SearchFirstOfK(vector<int>& A, int k) {
	int left = 0;
	int right = A.size() - 1;
	int res = -1;

	while (left <= right) {

		int mid = left + (right - left) / 2;

		if (A[mid] <  k) {
			right = mid - 1;
		}  else if (A[mid] == k) {
			res = mid;
			right = mid - 1;
		} else {
			left = mid + 1;
		}
	}

	return res;
}