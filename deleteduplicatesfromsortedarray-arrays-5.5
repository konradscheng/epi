int removeDuplicates(vector<int>& nums) {
    
    if (nums.size() == 0) {
        return 0;
    }
    
    int currSwapIndex = 1;
    int currElement = nums[0];
    
    for (int i = 1; i < nums.size(); i++) {
        if (nums[i] != currElement) {
            currElement = nums[i];
            swap(nums[i], nums[currSwapIndex++]);
        }
    }
    
    return currSwapIndex;
}